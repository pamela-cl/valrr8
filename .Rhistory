graph_error <- function(data, titles){
# Compute percentages
data$fraction = data$n / sum(data$n)
# Compute the cumulative percentages (top of each rectangle)
data$ymax = cumsum(data$fraction)
# Compute the bottom of each rectangle
data$ymin = c(0, head(data$ymax, n=-1))
# Make the plot
a <- ggplot(data, aes(ymax=ymax, ymin=ymin, xmax=4, xmin=3, fill=flag)) +
geom_rect() +
coord_polar(theta="y") + # Try to remove that to understand how the chart is built initially
xlim(c(2, 4)) +
scale_fill_manual(
values = c("#ff0000","#009933"),
name = "Número de columnas",
labels = c("Correctas","Incorrectas")
) +
labs(
title = "Columnas incorrectas",
subtitle = "Número de columnas con el formato incorrecto"
)+
theme_void()+
theme(
title = element_text(colour="black", size=16, face="bold")
)
)
